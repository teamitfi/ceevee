{
  "name": "cdk",
  "version": "0.1.0",
  "license": "MIT",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy:infra": "cdk deploy CeeveeEcrStack CeeveeNetworkStack CeeveeCognitoStack",
    "deploy:db": "cdk deploy CeeveeDbStack",
    "deploy:api": "cdk deploy CeeveeApiStack",
    "deploy:all": "cdk deploy --all",
    "destroy:api": "cdk destroy CeeveeApiStack",
    "destroy:db": "cdk destroy CeeveeDbStack",
    "destroy:infra": "cdk destroy CeeveeEcrStack CeeveeNetworkStack CeeveeCognitoStack",
    "db:list": "aws rds describe-db-instances --output table"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "2.179.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.179.0",
    "constructs": "^10.4.2"
  }
}
